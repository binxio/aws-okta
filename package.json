{
  "name": "aws-okta",
  "version": "0.0.1",
  "description": "",
  "homepage": "https://github.com/binxio/aws-okta",
  "main": "./dist/main.js",
  "build": {
    "appId": "com.electron.aws-okta.app",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "npm run build && node_modules/.bin/electron ./dist/main.js",
    "package:mac": "npm run build && electron-builder --mac",
    "package:win": "npm run build && electron-builder --win"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@types/ini": "^1.3.31",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.38",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "jest": "^28.1.1",
    "playwright": "^1.22.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.105.0",
    "@types/inquirer": "^8.2.1",
    "dotenv": "^16.0.1",
    "ini": "^3.0.0",
    "inquirer": "^8.2.4",
    "ts-node": "^10.8.1"
  }
}