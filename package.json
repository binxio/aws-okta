{
  "name": "aws-okta",
  "version": "0.0.1",
  "description": "",
  "homepage": "https://github.com/binxio/aws-okta",
  "main": "./dist/main.js",
  "scripts": {
    "test": "jest",
    "build": "esbuild --platform=node --bundle main.ts --outdir=dist --format=cjs --external:electron",
    "start": "npm run build && electron ./dist/main.js",
    "package:macos-latest": "npm run build && electron-builder --mac",
    "package:windows-latest": "npm run build && electron-builder --win",
    "package:ubuntu-latest": "npm run build && electron-builder --linux"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "sideEffects": false,
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.38",
    "electron-builder": "^23.0.3",
    "esbuild": "^0.14.46",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.105.0",
    "@types/inquirer": "^8.2.1",
    "ini": "^3.0.0",
    "inquirer": "^8.2.4",
    "ts-node": "^10.8.1"
  },
  "build": {
    "appId": "com.electron.aws-okta.app",
    "files": [
      "dist",
      "package.json",
      "!node_modules"
    ],
    "compression": "maximum",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "linux": {
      "category": "Development",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    }
  }
}